class ASTalternative extends ASTexpression {
    var condition;
    var consequence;
    var alternant;

    method getCondition () {
        return self.condition;
    }

    method getConsequence () {
        return self.consequence;
    }

    method getAlternant () {
        return self.alternant;
    }

    method isTernary () {
        return self.alternant != null;
    }

}