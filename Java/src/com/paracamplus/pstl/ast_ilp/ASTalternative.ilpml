class ASTalternative extends ASTexpression {
    var condition;
    var consequence;
    var alternant;

    method getCondition () 
    self.condition;
    

    method getConsequence () 
    self.consequence;
    

    method getAlternant () 
    self.alternant;
    

    method isTernary () 
    self.alternant != null;
    
    
    //method collectGlobalVariable()
    
}