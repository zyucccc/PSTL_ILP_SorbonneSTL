class ASTbinaryOperation extends ASTexpression{
    var operator;
    var leftOperand;
    var rightOperand;

    
	method getOperator() 
    self.operator;


	 method getOperands()
	 (
     init = new List(new NULL());
     init.add(leftOperand);
     init.add(rightOperand);
     )
     ;
    

	method getLeftOperand()
     self.leftOperand;
    

	method getRightOperand() 
     self.rightOperand;
    
    
}