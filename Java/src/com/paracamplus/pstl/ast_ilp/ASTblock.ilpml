class ASTblock extends ASTexpression{
    class ASTbinding extends AST{
        var variable;
        var initialisation;

        method getVariable () 
         self.variable;
        

		method getInitialisation () 
         self.initialisation;
        
    }

    var binding; 
    var body;

    method getBindings() 
        self.binding;
    
    
	method getBody() 
        self.body;
    
}